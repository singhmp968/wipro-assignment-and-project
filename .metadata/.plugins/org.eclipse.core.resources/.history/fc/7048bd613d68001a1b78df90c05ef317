package Assignment2;
import java.sql.*;
import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.SQLException;
import java.util.Date;

import Assignment1.DbUtil;

public class JdbcCall {

	 Connection conn=null;
PreparedStatement pstmt=null;
public JdbcCall(){
	DbUtil db = new DbUtil();
	conn=db.connectionFactory();
}
public  void insert(int rollno,String StudentName,String Standard,java.sql.Date udob,double fees) throws SQLException {
	String sql = "INSERT INTO studTab VALUES(?, ?, ?, ?, ?)";
	pstmt = conn.prepareStatement(sql);
	pstmt.setInt(1, rollno);
	pstmt.setString(2, StudentName);
	pstmt.setString(3, Standard);
	
	pstmt.setDate(4,udob);
	pstmt.setDouble(5, fees);
	pstmt.executeQuery();
	System.out.println("Student added successfully");

	
}




}
