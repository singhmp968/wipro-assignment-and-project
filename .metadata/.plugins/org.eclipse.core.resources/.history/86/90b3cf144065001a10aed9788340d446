
public class logic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int arr1[]= {1,2,3,4,5};
int arr2[]= {1,2,3,4,5};
int k=8;
for(int i=0;i<arr1.length;i++)
{
	while(k!=0) {
		for(int j=i+1;j<k;j++) {
			System.out.print(arr1[i]+""+arr2[j]);
		}System.out.println("");
	k=k-2;}
}
	}

}
